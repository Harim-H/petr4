# header hdr { bit<8>  e; bit<16> t; bit<8>  l; bit<8> r; bit<1>  v; }

add t_lpm h.h.key:0x11/0xF0 set_port(x:1)
add t_lpm h.h.key:0x12/0xFF set_port(x:2)

